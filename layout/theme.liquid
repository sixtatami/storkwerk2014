              {% if settings.search_enabled %}
                <li class="search">
                  <a href="/search" title="Search" class="icon-search" id="search-toggle"></a>
                </li>
              {% endif %}
              <li class="cart">
                <a href="#cart" class="icon-cart cart-button"><span>{{ cart.item_count }}</span></a>
              </li>
            </ul>

          </div>
        </div>
      </div>


